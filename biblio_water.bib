	Number = {{16}},
	Pages = {{7340-7347}},
	Times-Cited = {{8}},
	Title = {{Photochemistry of coronene with water at 10 K: first tentative identification by infrared spectroscopy of oxygen containing coronene products}},
	Unique-Id = {{ISI:000289203800011}},
	Volume = {{13}},
	Year = {{2011}},
